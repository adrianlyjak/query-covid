  "devDependencies": {
    "@types/react": "^16.9.31",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.4",
    "@types/express-http-proxy": "^1.5.12",
    "@types/express-promise-router": "^3.0.0",
    "@types/geojson": "^7946.0.7",
