    "express-promise-router": "^3.0.3",
    "highcharts": "^8.0.4",
    "leaflet": "^1.6.0",
    "moment": "^2.29.2",
    "papaparse": "^5.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-leaflet": "^2.6.3",
    "ts-node": "^8.8.1",
